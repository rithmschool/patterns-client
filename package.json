  "private": true,
  "dependencies": {
    "axios": "0.16.2",
    "bootstrap-grid": "2.0.1",
    "bootstrap-sass": "3.3.7",
    "jwt-decode": "2.2.0",
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-dnd": "2.5.1",
    "react-dnd-html5-backend": "2.5.1",
    "react-dom": "15.6.1",
    "react-redux": "5.0.6",
